{
  "name": "<%= name %>",
  "description": "<%= description %>",
  "private": true,
  "scripts": {
    "dev": "cooking watch -p",
    "dist": "cooking build -p",
    "qrcode": "node build/qrcode.js",
    "copy:ios": "cpy dist/weex.js platforms/ios/assets --rename=index.js",
    "copy:android": "cpy dist/weex.js android/app/src/main/assets --rename=index.js",
    "debug": "weex-devtool"
  },<% if (github) { %>
  "repository": {
    "type": "git",
    "url": "git+<%= github %>.git"
  },
  "homepage": "<%= github %>#readme",
  "bugs": {
    "url": "<%= github %>/issues"
  },<% } %><% if (author) { %>
  "author": "<%= author %>",<% } %>
  "license": "<%= license %>",
  "peerDependencies": {
    "weex-devtool": "^0.2.76"
  },
  "dependencies": {
    "vue": "^2.1.0",
    "vue-router": "^2.2.0",
    "weex-vue-render": "^0.1.8"
  },
  "devDependencies": {
    "cpy-cli": "*",
    "cooking-vue2": "*",
    "eslint-config-weex": "^0.1.5",
    "ip": "^1.1.4",
    "qrcode-terminal": "^0.11.0",
    "weex-loader": "^0.4.1"<% if (cooking) { %>,
    "cooking": "^1.0.0",
    "cooking-lint": "*",<% if (csstype) { %>
    "cooking-<%= csstype %>": "*",<% } %>
    <% if (js !== 'buble') { %>
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",<% } %>
    "css-loader": "^0.24.0",
    "extract-text-webpack-plugin": <% if (cooking == 1) { %>"^1.0.0"<% } else {%>"^2.0.0-beta"<% } %>,
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.9.0",
    "json-loader": "^0.5.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.0.0",
    "webpack-dev-server": "^2.3.0",
    "postcss": "^5.1.0",
    "postcss-loader": "^1.0.0"<% } %>
  }
}
